{
  "name": "scrollfolio-backup",
  "version": "1.0.0",
  "description": "Project backup and restore utilities",
  "scripts": {
    "backup": "node scripts/backup.js .",
    "restore": "node scripts/restore.js",
    "list-backups": "node -e \"const fs=require('fs');console.log(fs.readdirSync('./backups').filter(d=>fs.statSync('./backups/'+d).isDirectory()).join('\\n'))\""
  },
  "devDependencies": {
    "@types/node": "^22.14.0"
  }
}
